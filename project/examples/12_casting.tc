/* Type casting */

int main() {
    int i = 42;
    double d = 3.14;

    int i2 = cast<int>(d);
    double d2 = cast<double>(i);

    // Pointer casting
    void* generic_ptr = &i;
    int* int_ptr = cast<int*>(generic_ptr);

    // Cast between pointer types
    char* char_ptr = cast<char*>(int_ptr);

    return 0;
}